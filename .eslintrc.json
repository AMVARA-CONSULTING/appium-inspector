{
  "extends": "@appium/eslint-config-appium",
  "parser": "@babel/eslint-parser",
  "plugins": [
    "import",
    "react",
    "react-native",
    "@babel"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "@babel/arrow-parens": 0,
    "import/no-unresolved": [
      2,
      {
        "ignore": ["electron"]
      }
    ],
    "require-await": 2,
    "react-native/no-inline-styles": 2
  }
}
